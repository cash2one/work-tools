<root>
    <log prefix="gate_server" level="4" />
    <client_acceptor listen_ip="0.0.0.0" listen_port="9999" online_threshold="1000" />
    <server_acceptor listen_ip="0.0.0.0" listen_port="9998" />
    <log_server ip="127.0.0.1" port="8899" />
    <name_server ip="127.0.0.1" port="9950" />

    <!-- check表示是否做token验证， type = 1表示xy.com平台， type=1 key1表示私钥-->
    <token_check check="false" type="1" key1="test" key2="not used"/>
    <shutdown delay_sec="5" />
    <message_check threshold="true" seq="false" heartbeat="false" />

  <server_id_list>
    <server_id id="1" />
    <server_id id="2" />
  </server_id_list>
</root>

